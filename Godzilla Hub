        aimbotSliderButtonCorner.Parent = aimbotSliderButton
        
        local aimbotDragging = false
        aimbotSliderButton.MouseButton1Down:Connect(function()
            aimbotDragging = true
        end)
        
        UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                aimbotDragging = false
            end
        end)
        
        UserInputService.InputChanged:Connect(function(input)
            if aimbotDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                local mousePos = UserInputService:GetMouseLocation()
                local sliderPos = aimbotSliderBg.AbsolutePosition.X
                local sliderSize = aimbotSliderBg.AbsoluteSize.X
                local relativePos = math.clamp((mousePos.X - sliderPos) / sliderSize, 0, 1)
                
                aimbotSliderButton.Position = UDim2.new(relativePos, -8, 0.5, -8)
                aimbotSliderFill.Size = UDim2.new(relativePos, 0, 1, 0)
                
                aimbotFOV = math.floor(50 + (relativePos * 250))
                aimbotSliderLabel.Text = "FOV: " .. aimbotFOV
            end
        end)
        
