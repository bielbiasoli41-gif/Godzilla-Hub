-- Fun√ß√µes auxiliares
local function clearContent()
    for _, child in pairs(ContentContainer:GetChildren()) do
        if not child:IsA("UIListLayout") and not child:IsA("UIPadding") then
            child:Destroy()
        end
    end
end

local function createLabel(text, parent, textSize, bold)
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -20, 0, textSize or 20)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
    label.Font = bold and Enum.Font.GothamBold or Enum.Font.Gotham
    label.TextSize = textSize or 14
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.TextWrapped = true
    label.Parent = parent
    return label
end

local function createButton(text, callback, parent)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, -20, 0, 35)
    button.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
    button.BorderSizePixel = 0
    button.Text = text
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.GothamBold
    button.TextSize = 14
    button.Parent = parent
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 5)
    corner.Parent = button
    
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(0, 140, 255)
    end)
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
    end)
    button.MouseButton1Click:Connect(callback)
    
    return button
end

local function createImage(imageId, parent)
    local imageFrame = Instance.new("Frame")
    imageFrame.Size = UDim2.new(1, -20, 0, 200)
    imageFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    imageFrame.BorderSizePixel = 0
    imageFrame.Parent = parent
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = imageFrame
    
    local image = Instance.new("ImageLabel")
    image.Size = UDim2.new(1, 0, 1, 0)
    image.BackgroundTransparency = 1
    image.Image = "rbxassetid://" .. imageId
    image.ScaleType = Enum.ScaleType.Fit
    image.Parent = imageFrame
    
    local imageCorner = Instance.new("UICorner")
    imageCorner.CornerRadius = UDim.new(0, 8)
    imageCorner.Parent = image
    
    return imageFrame
end

local function createSeparator(parent)
    local separator = Instance.new("Frame")
    separator.Size = UDim2.new(1, -20, 0, 1)
    separator.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    separator.BorderSizePixel = 0
    separator.Parent = parent
    return separator
end

local function createSpacer(height, parent)
    local spacer = Instance.new("Frame")
    spacer.Size = UDim2.new(1, 0, 0, height or 10)
    spacer.BackgroundTransparency = 1
    spacer.Parent = parent
    return spacer
end

-- Vari√°veis para atualiza√ß√£o em tempo real
local timeLabel, fpsLabel, playerCountLabel

-- Vari√°veis globais para PvP
local aimbotEnabled = false
local aimbotFOV = 100
local aimbotCircle = nil
local hitboxEnabled = false
local hitboxSize = 5
local selectedPlayer = nil
local originalPosition = nil

-- Fun√ß√£o para criar c√≠rculo do Aimbot
local function createAimbotCircle()
    if aimbotCircle then
        aimbotCircle:Remove()
    end
    
    aimbotCircle = Drawing.new("Circle")
    aimbotCircle.Thickness = 2
    aimbotCircle.NumSides = 64
    aimbotCircle.Radius = aimbotFOV
    aimbotCircle.Filled = false
    aimbotCircle.Color = Color3.fromRGB(255, 255, 255)
    aimbotCircle.Transparency = 1
    aimbotCircle.Visible = true
    
    -- Atualizar posi√ß√£o do c√≠rculo
    RunService.RenderStepped:Connect(function()
        if aimbotCircle and aimbotEnabled then
            local camera = workspace.CurrentCamera
            local viewportSize = camera.ViewportSize
            aimbotCircle.Position = Vector2.new(viewportSize.X / 2, viewportSize.Y / 2)
            aimbotCircle.Radius = aimbotFOV
            aimbotCircle.Visible = true
        elseif aimbotCircle then
            aimbotCircle.Visible = false
        end
    end)
end

-- Fun√ß√£o Aimbot
local function getClosestPlayerInFOV()
    local camera = workspace.CurrentCamera
    local closestPlayer = nil
    local shortestDistance = aimbotFOV
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Head") then
            local head = player.Character.Head
            local screenPoint, onScreen = camera:WorldToViewportPoint(head.Position)
            
            if onScreen then
                local viewportSize = camera.ViewportSize
                local center = Vector2.new(viewportSize.X / 2, viewportSize.Y / 2)
                local mousePos = Vector2.new(screenPoint.X, screenPoint.Y)
                local distance = (center - mousePos).Magnitude
                
                if distance < shortestDistance then
                    shortestDistance = distance
                    closestPlayer = player
                end
            end
        end
    end
    
    return closestPlayer
end

-- Loop do Aimbot
spawn(function()
    RunService.RenderStepped:Connect(function()
        if aimbotEnabled then
            local targetPlayer = getClosestPlayerInFOV()
            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
                local camera = workspace.CurrentCamera
                camera.CFrame = CFrame.new(camera.CFrame.Position, targetPlayer.Character.Head.Position)
            end
        end
    end)
end)

-- Fun√ß√£o para expandir HitBox (apenas cabe√ßa)
local function expandHitbox(character, size)
    if character:FindFirstChild("Head") then
        local head = character.Head
        head.Size = Vector3.new(size, size, size)
        head.Transparency = 0.5
        head.CanCollide = false
        
        -- Manter a face vis√≠vel
        if head:FindFirstChild("face") then
            head.face.Transparency = 0
        end
    end
end

-- Fun√ß√£o para resetar HitBox
local function resetHitbox(character)
    if character:FindFirstChild("Head") then
        local head = character.Head
        head.Size = Vector3.new(2, 1, 1) -- Tamanho padr√£o da cabe√ßa
        head.Transparency = 0
        head.CanCollide = false
        
        if head:FindFirstChild("face") then
            head.face.Transparency = 0
        end
    end
end

-- Loop do HitBox
spawn(function()
    while wait(0.1) do
        if hitboxEnabled then
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    expandHitbox(player.Character, hitboxSize)
                end
            end
        else
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    resetHitbox(player.Character)
                end
            end
        end
    end
end)

-- Fun√ß√£o para matar jogador
local function killPlayer(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Godzilla Hub";
            Text = "Jogador inv√°lido!";
            Duration = 3;
        })
        return
    end
    
    -- Salvar posi√ß√£o original
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        originalPosition = LocalPlayer.Character.HumanoidRootPart.CFrame
    end
    
    -- Equipar Machete
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
        if tool.Name == "Machete" then
            LocalPlayer.Character.Humanoid:EquipTool(tool)
            break
        end
    end
    
    -- Teleportar e atacar
    spawn(function()
        for i = 1, 10 do
            if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                -- Teleportar atr√°s do jogador
                local targetPos = targetPlayer.Character.HumanoidRootPart.CFrame
                LocalPlayer.Character.HumanoidRootPart.CFrame = targetPos * CFrame.new(0, 0, 3)
                
                -- Clicar (simular ataque)
                wait(1)
                mouse1click()
            else
                break
            end
        end
        
        -- Voltar para posi√ß√£o original
        wait(0.5)
        if originalPosition and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            LocalPlayer.Character.HumanoidRootPart.CFrame = originalPosition
        end
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "Godzilla Hub";
            Text = "A√ß√£o conclu√≠da!";
            Duration = 3;
        })
    end)
end

-- Fun√ß√£o para atualizar conte√∫do baseado na aba
function updateContent(tabName)
    clearContent()
    
    if tabName == "Cr√©ditos" then
        -- URL do Discord
        createLabel("https://discord.gg/PDc8bHsT", ContentContainer, 16, true)
        createSpacer(10, ContentContainer)
        
        -- Imagem do Discord
        createImage("110586608819666", ContentContainer)
        createSpacer(10, ContentContainer)
        
        -- Label Discord Server
        createLabel("Discord Server:", ContentContainer, 14)
        
        -- Bot√£o Copy Link
        createButton("Copy Link", function()
            setclipboard("https://discord.gg/PDc8bHsT")
            game.StarterGui:SetCore("SendNotification", {
                Title = "Godzilla Hub";
                Text = "Link do Discord copiado!";
                Duration = 3;
            })
        end, ContentContainer)
        
        createSpacer(15, ContentContainer)
        createSeparator(ContentContainer)
        createSpacer(15, ContentContainer)
        
        -- Informa√ß√µes do Script
        createLabel("Informa√ß√µes do Script", ContentContainer, 16, true)
        createLabel("", ContentContainer, 5)
        createLabel("Criadores:", ContentContainer, 14)
        createLabel("  sereireconhecido", ContentContainer, 14)
        createLabel("  Godzilla_eoking1", ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("By:", ContentContainer, 14)
        createLabel("  Godzilla Studios", ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("Voc√™ est√° usando:", ContentContainer, 14)
        createLabel("  Godzilla Hub", ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("Executor Utilizado:", ContentContainer, 14)
        createLabel("  " .. (identifyexecutor and identifyexecutor() or "Nome do Executor"), ContentContainer, 14)
        
        createSpacer(15, ContentContainer)
        createSeparator(ContentContainer)
        createSpacer(15, ContentContainer)
        
        -- Informa√ß√µes
        createLabel("Informa√ß√µes", ContentContainer, 16, true)
        createLabel("", ContentContainer, 5)
        createLabel("Hoje √© dia:", ContentContainer, 14)
        createLabel("  " .. os.date("(%d/%m/%Y - %H:%M:%S)"), ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("Seu Nickname:", ContentContainer, 14)
        createLabel("  " .. LocalPlayer.Name, ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("Seu ID:", ContentContainer, 14)
        createLabel("  " .. LocalPlayer.UserId, ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("Seu Jogo:", ContentContainer, 14)
        local gameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
        createLabel("  " .. gameName, ContentContainer, 14)
        createLabel("", ContentContainer, 5)
        createLabel("Tempo de uso do script:", ContentContainer, 14)
        
        -- Label de tempo que ser√° atualizada
        local timeUsageLabel = createLabel("  (Tempo de uso do script)", ContentContainer, 14)
        
        -- Atualizar tempo em tempo real
        spawn(function()
            while ContentContainer and timeUsageLabel and timeUsageLabel.Parent do
                wait(1)
                local elapsed = tick() - startTime
                local hours = math.floor(elapsed / 3600)
                local minutes = math.floor((elapsed % 3600) / 60)
                local seconds = math.floor(elapsed % 60)
                
                if timeUsageLabel and timeUsageLabel.Parent then
                    timeUsageLabel.Text = string.format("  %02d:%02d:%02d", hours, minutes, seconds)
                else
                    break
                end
            end
        end)
        
        createLabel("", ContentContainer, 5)
        createLabel("Seu Fps:", ContentContainer, 14)
        fpsLabel = createLabel("  (Fps do jogador)", ContentContainer, 14)
        
        -- Atualizar FPS em tempo real
        spawn(function()
            local lastUpdate = tick()
            local frames = 0
            
            local connection
            connection = RunService.RenderStepped:Connect(function()
                frames = frames + 1
                
                if tick() - lastUpdate >= 1 then
                    if fpsLabel and fpsLabel.Parent then
                        fpsLabel.Text = "  " .. frames .. " FPS"
                    else
                        connection:Disconnect()
                    end
                    frames = 0
                    lastUpdate = tick()
                end
            end)
        end)
        
        createLabel("", ContentContainer, 5)
        createLabel("Quantidade de jogadores no serv:", ContentContainer, 14)
        playerCountLabel = createLabel("  (quantidade de jogadores)", ContentContainer, 14)
        
        -- Atualizar contagem de jogadores inicialmente
        if playerCountLabel then
            playerCountLabel.Text = "  " .. #Players:GetPlayers() .. " jogadores"
        end
        
        -- Atualizar contagem de jogadores
        Players.PlayerAdded:Connect(function()
            wait(0.1)
            if playerCountLabel and playerCountLabel.Parent then
                playerCountLabel.Text = "  " .. #Players:GetPlayers() .. " jogadores"
            end
        end)
        
        Players.PlayerRemoving:Connect(function()
            wait(0.1)
            if playerCountLabel and playerCountLabel.Parent then
                playerCountLabel.Text = "  " .. #Players:GetPlayers() .. " jogadores"
            end
        end)
        
    elseif tabName == "PvP" then
        -- T√≠tulo da aba
        createLabel("Aimbot", ContentContainer, 18, true)
        createSpacer(10, ContentContainer)
        
        -- Toggle Aimbot
        local aimbotToggle = Instance.new("Frame")
        aimbotToggle.Size = UDim2.new(1, -20, 0, 40)
        aimbotToggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        aimbotToggle.BorderSizePixel = 0
        aimbotToggle.Parent = ContentContainer
        
        local aimbotCorner = Instance.new("UICorner")
        aimbotCorner.CornerRadius = UDim.new(0, 5)
        aimbotCorner.Parent = aimbotToggle
        
        local aimbotLabel = Instance.new("TextLabel")
        aimbotLabel.Size = UDim2.new(0.7, 0, 1, 0)
        aimbotLabel.Position = UDim2.new(0, 10, 0, 0)
        aimbotLabel.BackgroundTransparency = 1
        aimbotLabel.Text = "Aimbot"
        aimbotLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        aimbotLabel.Font = Enum.Font.GothamBold
        aimbotLabel.TextSize = 14
        aimbotLabel.TextXAlignment = Enum.TextXAlignment.Left
        aimbotLabel.Parent = aimbotToggle
        
        local aimbotStatus = Instance.new("TextLabel")
        aimbotStatus.Size = UDim2.new(0.3, -10, 1, 0)
        aimbotStatus.Position = UDim2.new(0.7, 0, 0, 0)
        aimbotStatus.BackgroundTransparency = 1
        aimbotStatus.Text = "Off"
        aimbotStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        aimbotStatus.Font = Enum.Font.GothamBold
        aimbotStatus.TextSize = 14
        aimbotStatus.TextXAlignment = Enum.TextXAlignment.Right
        aimbotStatus.Parent = aimbotToggle
        
        local aimbotButton = Instance.new("TextButton")
        aimbotButton.Size = UDim2.new(1, 0, 1, 0)
        aimbotButton.BackgroundTransparency = 1
        aimbotButton.Text = ""
        aimbotButton.Parent = aimbotToggle
        
        aimbotButton.MouseButton1Click:Connect(function()
            aimbotEnabled = not aimbotEnabled
            if aimbotEnabled then
                aimbotStatus.Text = "On"
                aimbotStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
                createAimbotCircle()
            else
                aimbotStatus.Text = "Off"
                aimbotStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
                if aimbotCircle then
                    aimbotCircle:Remove()
                    aimbotCircle = nil
                end
            end
        end)
        
        createSpacer(10, ContentContainer)
        
        -- Slider Aimbot FOV
        local aimbotSlider = Instance.new("Frame")
        aimbotSlider.Size = UDim2.new(1, -20, 0, 50)
        aimbotSlider.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        aimbotSlider.BorderSizePixel = 0
        aimbotSlider.Parent = ContentContainer
        
        local aimbotSliderCorner = Instance.new("UICorner")
        aimbotSliderCorner.CornerRadius = UDim.new(0, 5)
        aimbotSliderCorner.Parent = aimbotSlider
        
        local aimbotSliderLabel = Instance.new("TextLabel")
        aimbotSliderLabel.Size = UDim2.new(1, -20, 0, 20)
        aimbotSliderLabel.Position = UDim2.new(0, 10, 0, 5)
        aimbotSliderLabel.BackgroundTransparency = 1
        aimbotSliderLabel.Text = "FOV: " .. aimbotFOV
        aimbotSliderLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        aimbotSliderLabel.Font = Enum.Font.Gotham
        aimbotSliderLabel.TextSize = 12
        aimbotSliderLabel.TextXAlignment = Enum.TextXAlignment.Left
        aimbotSliderLabel.Parent = aimbotSlider
        
        local aimbotSliderBg = Instance.new("Frame")
        aimbotSliderBg.Size = UDim2.new(1, -20, 0, 6)
        aimbotSliderBg.Position = UDim2.new(0, 10, 0, 32)
        aimbotSliderBg.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        aimbotSliderBg.BorderSizePixel = 0
        aimbotSliderBg.Parent = aimbotSlider
        
        local aimbotSliderBgCorner = Instance.new("UICorner")
        aimbotSliderBgCorner.CornerRadius = UDim.new(0, 3)
        aimbotSliderBgCorner.Parent = aimbotSliderBg
        
