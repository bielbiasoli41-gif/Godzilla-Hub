        aimbotSliderFill.Size = UDim2.new(0.5, 0, 1, 0)
        aimbotSliderFill.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
        aimbotSliderFill.BorderSizePixel = 0
        aimbotSliderFill.Parent = aimbotSliderBg
        
