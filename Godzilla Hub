        -- Bot√£o Roubar
        createButton("Roubar ‚ûú", function()
            if selectedRoubarPlayer and selectedRoubarPlayer.Character and selectedRoubarPlayer.Character:FindFirstChild("HumanoidRootPart") then
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    -- Salvar posi√ß√£o original
                    local originalPosition = LocalPlayer.Character.HumanoidRootPart.CFrame
                    
                    -- Iniciar processo de roubar
                    spawn(function()
                        local vim = game:GetService("VirtualInputManager")
                        local startTime = tick()
                        local stopRobar = false
                        
                        -- Thread para manter F pressionado
                        spawn(function()
                            while not stopRobar do
                                vim:SendKeyEvent(true, Enum.KeyCode.F, false, game)
                                wait(0.1)
                            end
                            vim:SendKeyEvent(false, Enum.KeyCode.F, false, game)
                        end)
                        
                        -- Teleportar a cada 1 segundo por 4 segundos
                        while tick() - startTime < 4 do
                            -- Teleportar atr√°s do jogador
                            if selectedRoubarPlayer.Character and selectedRoubarPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                local targetPos = selectedRoubarPlayer.Character.HumanoidRootPart.CFrame
                                LocalPlayer.Character.HumanoidRootPart.CFrame = targetPos * CFrame.new(0, 0, 3)
                            end
                            
                            wait(1)
                        end
                        
                        -- Parar de pressionar F
                        stopRobar = true
                        wait(0.2)
                        
                        -- Voltar para posi√ß√£o original
                        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            LocalPlayer.Character.HumanoidRootPart.CFrame = originalPosition
                        end
                        
                        game.StarterGui:SetCore("SendNotification", {
                            Title = "Godzilla Hub";
                            Text = "Roubo conclu√≠do!";
                            Duration = 3;
                        })
                    end)
                    
                    game.StarterGui:SetCore("SendNotification", {
                        Title = "Godzilla Hub";
                        Text = "Roubando " .. selectedRoubarPlayer.Name .. "...";
                        Duration = 3;
                    })
                end
            else
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Godzilla Hub";
                    Text = "Selecione um jogador primeiro!";
                    Duration = 3;
                })
            end
        end, ContentContainer)
        
