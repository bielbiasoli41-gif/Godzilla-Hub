                    local tempoFim = tempoInicio + segundos
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
                    while tick() < tempoFim and scriptAtivo do
                        task.wait(0.05)
                    end
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.F, false, game)
                end

                local function executarSequencia()
                    scriptAtivo = true
                    botaoIniciar.Text = "Executando..."
                    botaoIniciar.BackgroundColor3 = Color3.fromRGB(150, 150, 0)
                    
                    for indiceCofre = 1, #cofres do
                        if not scriptAtivo then break end
                        local coordCofre = cofres[indiceCofre]
                        statusProgresso.Text = "Progresso: " .. (indiceCofre - 1) .. "/" .. #cofres .. " cofres"
