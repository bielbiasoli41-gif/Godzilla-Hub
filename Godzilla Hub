local function ativarGodMode()
    if not godModeAtivado then
        humanoid.MaxHealth = math.huge
        humanoid.Health = math.huge
        humanoid.HealthChanged:Connect(function(health)
            if godModeAtivado and health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end)
        godModeAtivado = true
        statusGodMode.Text = "God Mod: Ativado ‚úì"
        statusGodMode.TextColor3 = Color3.fromRGB(100, 255, 100)
    end
end
local function teleportar(posicao, nomeLugar)
    if character and humanoidRootPart then
        humanoid.WalkSpeed = 0
        task.wait(0.1)
        humanoidRootPart.CFrame = CFrame.new(posicao)
        humanoidRootPart.Anchored = true
        task.wait(0.2)
        humanoidRootPart.Anchored = false
        humanoid.WalkSpeed = 16
    end
end
local function manterFPressionada(segundos)
    local VirtualInputManager = game:GetService("VirtualInputManager")
    local tempoInicio = tick()
    local tempoFim = tempoInicio + segundos
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
    while tick() < tempoFim and scriptAtivo do task.wait(0.05) end
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.F, false, game)
end
local function executarSequencia()
    scriptAtivo = true
    botaoIniciar.Text = "Executando..."
    botaoIniciar.BackgroundColor3 = Color3.fromRGB(150, 150, 0)
    for indiceCofre = 1, #cofres do
        if not scriptAtivo then break end
        local coordCofre = cofres[indiceCofre]
        statusProgresso.Text = "Progresso: " .. (indiceCofre - 1) .. "/" .. #cofres .. " cofres"
        statusLocal.Text = "Local: Indo para Cofre #" .. indiceCofre
        teleportar(coordCofre, "Cofre #" .. indiceCofre)
        task.wait(1.5)
        statusLocal.Text = "Local: Cofre #" .. indiceCofre .. " (Coletando...)"
        manterFPressionada(5)
        task.wait(1)
        manterFPressionada(10)
        statusProgresso.Text = "Progresso: " .. indiceCofre .. "/" .. #cofres .. " cofres"
        statusLocal.Text = "Local: Voltando ao Acampamento..."
        teleportar(acampamento, "Acampamento")
        task.wait(1.5)
        statusLocal.Text = "Local: Acampamento"
    end
    if scriptAtivo then
        statusProgresso.Text = "Completo: " .. #cofres .. "/" .. #cofres .. " cofres ‚úì"
        statusProgresso.TextColor3 = Color3.fromRGB(100, 255, 100)
        statusLocal.Text = "Local: Farm Conclu√≠do!"
    end
    scriptAtivo = false
    botaoIniciar.Text = "Iniciar"
    botaoIniciar.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
end
botaoIniciar.MouseButton1Click:Connect(function()
    if not scriptAtivo then
        statusProgresso.Text = "Progresso: 0/" .. #cofres .. " cofres"
        statusProgresso.TextColor3 = Color3.fromRGB(200, 200, 200)
        statusLocal.Text = "Local: Iniciando..."
        task.spawn(executarSequencia)
    end
end)
