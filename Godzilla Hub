                        teleportar(coordCofre, "Cofre #" .. indiceCofre)
                        task.wait(1.5)
                        statusLocal.Text = "Local: Cofre #" .. indiceCofre .. " (Coletando...)"
                        manterFPressionada(5)
                        task.wait(1)
                        manterFPressionada(10)
                        statusProgresso.Text = "Progresso: " .. indiceCofre .. "/" .. #cofres .. " cofres"
                        statusLocal.Text = "Local: Voltando ao Acampamento..."
                        teleportar(acampamento, "Acampamento")
                        task.wait(1.5)
                        statusLocal.Text = "Local: Acampamento"
                    end
                    
                    if scriptAtivo then
                        statusProgresso.Text = "Completo: " .. #cofres .. "/" .. #cofres .. " cofres ‚úì"
                        statusProgresso.TextColor3 = Color3.fromRGB(100, 255, 100)
                        statusLocal.Text = "Local: Farm Conclu√≠do!"
                    end
                    
                    scriptAtivo = false
                    botaoIniciar.Text = "Iniciar"
                    botaoIniciar.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
                end

                botaoIniciar.MouseButton1Click:Connect(function()
                    if not scriptAtivo then
                        statusProgresso.Text = "Progresso: 0/" .. #cofres .. " cofres"
                        statusProgresso.TextColor3 = Color3.fromRGB(200, 200, 200)
                        statusLocal.Text = "Local: Iniciando..."
                        task.spawn(executarSequencia)
                    end
                end)

                botaoParar.MouseButton1Click:Connect(function()
                    if scriptAtivo then
                        scriptAtivo = false
                        botaoIniciar.Text = "Iniciar"
                        botaoIniciar.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
                        statusLocal.Text = "Local: Farm Parado"
                    end
                end)

                ativarGodMode()
            end)
        end, ContentContainer)
        
    else
        -- Outras abas (em desenvolvimento)
        createLabel("Em desenvolvimento...", ContentContainer, 16, true)
    end
    
    -- Ajustar tamanho do canvas
    local contentSize = 0
    for _, child in pairs(ContentContainer:GetChildren()) do
        if child:IsA("GuiObject") then
            contentSize = contentSize + child.Size.Y.Offset + 5
        end
    end
    ContentContainer.CanvasSize = UDim2.new(0, 0, 0, contentSize + 20)
end

-- Fun√ß√£o para criar bot√£o de aba
local function createTabButton(tabData, index)
    local button = Instance.new("TextButton")
    button.Name = tabData.name .. "Tab"
    button.Size = UDim2.new(1, 0, 0, 35)
    button.Position = UDim2.new(0, 0, 0, (index - 1) * 35)
    button.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    button.BorderSizePixel = 0
    button.Text = tabData.icon .. " " .. tabData.name
    button.TextColor3 = Color3.fromRGB(200, 200, 200)
    button.Font = Enum.Font.Gotham
    button.TextSize = 14
    button.TextXAlignment = Enum.TextXAlignment.Left
    
    -- Adicionar padding
    local padding = Instance.new("UIPadding")
    padding.PaddingLeft = UDim.new(0, 10)
    padding.Parent = button
    
    button.Parent = TabContainer
    
    -- Evento de clique
    button.MouseButton1Click:Connect(function()
        currentTab = tabData.name
        -- Animar indicador
        TweenService:Create(TabIndicator, TweenInfo.new(0.2), {Position = UDim2.new(1, -3, 0, (index - 1) * 35)}):Play()
        
        -- Atualizar cores dos bot√µes
        for _, child in pairs(TabContainer:GetChildren()) do
            if child:IsA("TextButton") then
                child.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
                child.TextColor3 = Color3.fromRGB(200, 200, 200)
            end
        end
        button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        
        -- Atualizar conte√∫do
        updateContent(tabData.name)
    end)
    
    button.MouseEnter:Connect(function()
        if currentTab ~= tabData.name then
            button.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        end
    end)
    
    button.MouseLeave:Connect(function()
        if currentTab ~= tabData.name then
            button.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        end
    end)
    
    return button
end

-- Criar todos os bot√µes de aba
for i, tab in ipairs(tabs) do
    createTabButton(tab, i)
end

-- Selecionar primeira aba
local creditosTab = TabContainer:FindFirstChild("Cr√©ditosTab")
if creditosTab then
    creditosTab.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    creditosTab.TextColor3 = Color3.fromRGB(255, 255, 255)
end

-- Layout para organizar elementos
local listLayout = Instance.new("UIListLayout")
listLayout.Padding = UDim.new(0, 5)
listLayout.SortOrder = Enum.SortOrder.LayoutOrder
listLayout.Parent = ContentContainer

-- Padding
local padding = Instance.new("UIPadding")
padding.PaddingTop = UDim.new(0, 10)
padding.PaddingLeft = UDim.new(0, 10)
padding.PaddingRight = UDim.new(0, 10)
padding.PaddingBottom = UDim.new(0, 10)
padding.Parent = ContentContainer

-- Carregar conte√∫do inicial
updateContent("Cr√©ditos")

-- Notifica√ß√£o de carregamento
game.StarterGui:SetCore("SendNotification", {
    Title = "Godzilla Hub";
    Text = "Script carregado com sucesso!";
    Duration = 5;
})

