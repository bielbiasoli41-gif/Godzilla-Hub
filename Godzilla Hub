                            roubarDropdownLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                            roubarDropdownOpen = false
                            roubarDropdownList.Visible = false
                            roubarDropdownList.Size = UDim2.new(1, 0, 0, 0)
                        end)
                    end
                end
                
                roubarDropdownList.CanvasSize = UDim2.new(0, 0, 0, playerCount * 32)
                local listHeight = math.min(playerCount * 32, 150)
                roubarDropdownList.Size = UDim2.new(1, 0, 0, listHeight)
                roubarDropdownList.Visible = true
            else
                roubarDropdownList.Visible = false
                roubarDropdownList.Size = UDim2.new(1, 0, 0, 0)
            end
        end)
        
